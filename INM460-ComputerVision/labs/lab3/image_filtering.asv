% Task 1: Smoothing
clear all;
clc;

% img = londonEye;
img = imread('LondonEye.jpg');
imgG = rgb2gray(img);
imgd = im2double(imgG); % imgd in [0,1]
f = ones(3,3)/9;
% f = ones(3,3)/18; % darker
% f = ones(3,3)/4; % lighter
img1 = imfilter(imgd,f);
figure;
subplot(121); imshow(imgd);
subplot(122); imshow(img1);
linkaxes;

